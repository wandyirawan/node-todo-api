{
    "extends": "airbnb-base",
    "plugins": [
      "mocha"
    ],
    "rules": {
      "consistent-return":0,
      "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
      "no-shadow": 0,
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "mocha/no-exclusive-tests": "error"
    }
}
